{
  "name": "kmscript",
  "displayName": "Mohe Script KM File Language Support",
  "description": "The Mohe Km File Scripting Language For VSCode",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.93.0"
  },
  "publisher": "Crainax",
  "repository": {
    "type": "git",
    "url": "https://github.com/KMScript/KMScript.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "kmscript",
        "aliases": [
          "KMScript",
          "kmscript"
        ],
        "extensions": [
          ".km"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kmscript",
        "scopeName": "source.kmscript",
        "path": "./syntaxes/kmscript.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "kmscript.format",
        "title": "Format KMScript"
      }
    ]
  },
  "icon": "icon.png",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^9.9.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}